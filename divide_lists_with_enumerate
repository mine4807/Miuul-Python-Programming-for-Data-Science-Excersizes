def divide_list(the_list):
    lists=[[],[]]
    for i, element in enumerate(the_list):
        if i % 2 == 0:
            lists[0].append(element)
        else:
            lists[1].append(element)
            
    return lists
            
the_list=[00,11,22,33,44,55]
print(divide_list(the_list))
